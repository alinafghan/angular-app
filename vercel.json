{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist/angular-app/browser"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "index.html"
      }
    ]
  }